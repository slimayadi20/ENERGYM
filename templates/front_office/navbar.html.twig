<header class="main-header header-style-one">

    <!-- Header Top -->
    <div class="header-top">
        <div class="outer-container">
            <div class="clearfix">

                <!-- Top Left -->
                <div class="top-left pull-left">
                    <div class="text">Welcome to ENER GYM Fitness</div>
                </div>

                <!-- Top Right -->
                <div class="top-right pull-right">

                    <!-- Info List -->
                    <ul class="info-list">
