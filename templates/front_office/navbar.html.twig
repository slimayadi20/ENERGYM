                                    {% endif %}

                                    {% if app.user  %}
                                        {% for role in app.user.roles %}
                                            {% if role != "ROLE_USER" %}
                                                <li><a href="{{ path('dashboard') }}">dashboard</a></li>
                                            {% endif %}
                                        {% endfor %}
                                    {% endif %}
                                    <div class="dropdown-btn"><span class="fa fa-angle-down"></span></div></li>
                                </ul>
                        </ul>
                    </div>
                </nav>

